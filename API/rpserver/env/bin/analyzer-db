#!/home/thuger/Oleg/Programming/Projects/rpserver/API/rpserver/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'rpserver','console_scripts','analyzer-db'
__requires__ = 'rpserver'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rpserver', 'console_scripts', 'analyzer-db')()
    )
